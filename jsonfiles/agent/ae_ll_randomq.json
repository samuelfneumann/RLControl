{
    "agent": "ActorCritic",
    "sweeps": {
        "critic_update": [ "sampled"],
        "actor_update": ["ll"],

        "norm_type": ["input_norm"],

        "exploration_policy": ["none"],

        "shared_l1_dim": [200],
        "actor_l2_dim": [200],
        "critic_l2_dim": [200],

        "actor_lr": [1e-2],
        "critic_lr": [1e-4],

        "rho": [0.05],
        "num_samples": [120],
        "num_modal": [1],

        "sample_for_eval": ["False"],
	    "equal_modal_selection": ["False"],
        "use_true_q": ["False"],

        "add_entropy": ["True"],
        "entropy_scale": [0.1]
    }
}
